start-at https://github.com/settings/security
timeout 30000
user-agent "Mozilla/5.0 (X11 Chrome/60.0.3112.113 Safari/537.36"

new-action
sleep 2000
fill #login_field [#USERNAME#]
sleep 1000

new-action
fill #password [#CURRENT_PASSWORD#]
click input[type=submit]
sleep 3000

new-action
fill #user_old_password [#CURRENT_PASSWORD#]
fill #user_new_password [#NEW_PASSWORD#]
fill #user_confirm_new_password [#NEW_PASSWORD#]
jclick button[type=submit]
sleep 3000

new-action
test-location https://github.com/settings/security
#test-selector button[class="flash-close js-flash-close"]
